{
  "name": "Too Many Tabs",
  "version": "0.8",
  "description": "Jump to the right tab immediately by clicking the title in the popup, even if you have 100 tabs.",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
      "default_title": "Too Many Tabs",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
